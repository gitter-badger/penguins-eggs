{
  "bin": {
    "teggs": "./build/index.js"
  },
  "author": "Piero Proietti",
  "name": "teggs",
  "version": "0.5.6",
  "description": "remaster your system and distribuite it by netboot or iso",
  "main": "./build/index.js",
  "scripts": {
    "start": "babel-node src",
    "build": "rollup -c rollup.config.js",
    "build-dev": "rollup -c rollup.config-dev.js",
    "compile": "nexe -i build/teggs.js -o bin/teggs",
    "prepublish": "npm run build"
  },
  "keywords": [
    "remastering",
    "pxe-server",
    "pxe-boot",
    "iso",
    "backup",
    "systemback"
  ],
  "homepage": "https://github.com/pieroproietti/teggs",
  "repository": {
    "type": "git",
    "url": "https://github.com/pieroproietti/teggs"
  },
  "license": "MIT",
  "dependencies": {
    "commander": "^2.11.0",
    "drivelist": "^5.1.8",
    "getos": "^3.0.1",
    "ip": "^1.1.5",
    "network": "^0.3.2",
    "shelljs": "^0.7.8",
    "source-map-support": "^0.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "nexe": "^2.0.0-beta.7",
    "rollup": "^0.48.2",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-shebang": "^0.1.4",
    "xml2js": "^0.4.19"
  }
}
